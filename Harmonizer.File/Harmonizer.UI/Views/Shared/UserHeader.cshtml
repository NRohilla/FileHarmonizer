@model Harmonizer.Core.Model.CommanUserData
<div class="well mb-3">
    <div class="float-left mr-2" id="divHarmonizer">
        <strong class="mr-2">File Harmonizer #:</strong><span id="lblHarmonizerId">@Model.HarmonizerValue</span>
    </div>
    @if (Model.BPType.ToLower().Trim() != "ind" && Model.BPType.ToLower().Trim() != "pind")
    {
        <div class="float-left mr-2" id="divSectfor">
            <strong class="mr-2">Sector :</strong><span id="lblSectporId">@Model.Sector</span>
        </div>

    }
    <div class="float-left mr-2" id="divBPID">
        <strong class="mr-2">BP #:</strong><span id="lblBPId">@Model.BPID</span>
    </div>
    <div class="float-left ml-5" id="divUserID">
        <strong class="mr-2">User ID :</strong> <span id="lblUserId">@Model.UserID</span>
    </div>

</div>

