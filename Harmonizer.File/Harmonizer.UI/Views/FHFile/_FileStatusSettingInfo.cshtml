<div class="row">
    <div class="col-md-4">
        <div class="w-100 float-left">
            <div class="float-left mr-2"><label> Source Folder:</label></div>
            <div class="float-left mr-2"><label >@Session["BPID"]\Template</label></div>
        </div>
        <div class="w-100 float-left">
            <div class="float-left mr-2"><label >Target Folder:</label></div>
            <div class="float-left mr-2"><label>@Session["BPID"]\Target</label></div>
        </div>
    </div>
    <div class="col-md-4" id="filterTempateFileinfo">
        <div class="w-100 float-left">
            <div class="float-left mr-2" style="width:35px;"><label >Filter:</label></div>
            <div class="float-left mr-2" style="width: 285px; word-break:break-all;"><label id="lblFilterName"></label></div>
        </div>
        <div class="w-100 float-left">
            <div class="float-left mr-2"  style="width:50px;"><label >Template:</label></div>
            <div class="float-left mr-2" style="width: 270px; word-break:break-all;"><label id="lblTemplateName"></label></div>
        </div>
    </div>
    <div class="col-md-4">
        <div class="w-100 float-left">
            <div class="float-left mr-2"><label >Harmonization #:</label></div>
            <div class="float-left mr-2"><label id="lblFHNumber"></label></div>
        </div>
        @*<div class="w-100 float-left">
            <div class="float-left mr-2">Print Harmonize Template</div>
            <div class="float-left mr-2"></div>
        </div>*@
    </div>
</div>

